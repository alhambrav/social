<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.WebView?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="org.craftercms.social.migration.controllers.MainController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem fx:id="mnuStart" mnemonicParsing="false" text="Start"/>
                        <MenuItem id="saveLog" fx:id="saveLog" mnemonicParsing="false" text="Save Log"/>
                        <MenuItem id="quit" fx:id="mnuQuit" mnemonicParsing="false" text="Quit"/>
                    </items>
                </Menu>

            </menus>
        </MenuBar>
        <SplitPane dividerPositions="0.5" maxHeight="-1.0" maxWidth="-1.0" orientation="VERTICAL" prefHeight="464.0"
                   prefWidth="674.0" VBox.vgrow="ALWAYS">
            <items>
                <AnchorPane maxHeight="200.0" minHeight="200.0" minWidth="200.0" prefWidth="800.0">
                    <children>
                        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE"
                                 AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0"
                                 AnchorPane.topAnchor="0" VBox.vgrow="ALWAYS">
                            <tabs>
                                <Tab text="Configuration">
                                    <content>
                                        <Accordion>
                                            <panes>
                                                <TitledPane animated="false" text="Source DB Connection">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0">
                                                            <children>
                                                                <GridPane layoutX="44.0" prefHeight="114.0"
                                                                          prefWidth="796.0"
                                                                          AnchorPane.bottomAnchor="0.0"
                                                                          AnchorPane.leftAnchor="0.0"
                                                                          AnchorPane.rightAnchor="0.0"
                                                                          AnchorPane.topAnchor="0.0">
                                                                    <columnConstraints>
                                                                        <ColumnConstraints hgrow="SOMETIMES"
                                                                                           minWidth="10.0"
                                                                                           prefWidth="100.0"/>
                                                                        <ColumnConstraints hgrow="SOMETIMES"
                                                                                           minWidth="10.0"
                                                                                           prefWidth="100.0"/>
                                                                    </columnConstraints>
                                                                    <rowConstraints>
                                                                        <RowConstraints minHeight="10.0"
                                                                                        prefHeight="30.0"
                                                                                        vgrow="SOMETIMES"/>
                                                                        <RowConstraints minHeight="10.0"
                                                                                        prefHeight="30.0"
                                                                                        vgrow="SOMETIMES"/>
                                                                        <RowConstraints minHeight="10.0"
                                                                                        prefHeight="30.0"
                                                                                        vgrow="SOMETIMES"/>
                                                                    </rowConstraints>
                                                                    <children>
                                                                        <TextField fx:id="srcHost" text="localhost"
                                                                                   GridPane.columnIndex="1"/>
                                                                        <TextField fx:id="srcPort" text="27017"
                                                                                   GridPane.columnIndex="1"
                                                                                   GridPane.rowIndex="1"/>
                                                                        <TextField fx:id="srcDb" text="crafter23"
                                                                                   GridPane.columnIndex="1"
                                                                                   GridPane.rowIndex="2"/>
                                                                        <Label text="Host"/>
                                                                        <Label text="Port" GridPane.rowIndex="1"/>
                                                                        <Label text="Db" GridPane.rowIndex="2"/>
                                                                    </children>
                                                                </GridPane>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </TitledPane>
                                                <TitledPane animated="false" text="Destination DB Connection">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0">
                                                            <children>
                                                                <GridPane layoutX="44.0" prefHeight="114.0"
                                                                          prefWidth="796.0"
                                                                          AnchorPane.bottomAnchor="0.0"
                                                                          AnchorPane.leftAnchor="0.0"
                                                                          AnchorPane.rightAnchor="0.0"
                                                                          AnchorPane.topAnchor="0.0">
                                                                    <columnConstraints>
                                                                        <ColumnConstraints hgrow="SOMETIMES"
                                                                                           minWidth="10.0"
                                                                                           prefWidth="100.0"/>
                                                                        <ColumnConstraints hgrow="SOMETIMES"
                                                                                           minWidth="10.0"
                                                                                           prefWidth="100.0"/>
                                                                    </columnConstraints>
                                                                    <rowConstraints>
                                                                        <RowConstraints minHeight="10.0"
                                                                                        prefHeight="30.0"
                                                                                        vgrow="SOMETIMES"/>
                                                                        <RowConstraints minHeight="10.0"
                                                                                        prefHeight="30.0"
                                                                                        vgrow="SOMETIMES"/>
                                                                        <RowConstraints minHeight="10.0"
                                                                                        prefHeight="30.0"
                                                                                        vgrow="SOMETIMES"/>
                                                                    </rowConstraints>
                                                                    <children>
                                                                        <TextField fx:id="dstHost" text="localhost"
                                                                                   GridPane.columnIndex="1"/>
                                                                        <TextField fx:id="dstPort" text="27017"
                                                                                   GridPane.columnIndex="1"
                                                                                   GridPane.rowIndex="1"/>
                                                                        <TextField fx:id="dstDb" text="crafter25"
                                                                                   GridPane.columnIndex="1"
                                                                                   GridPane.rowIndex="2"/>
                                                                        <Label text="Host"/>
                                                                        <Label text="Port" GridPane.rowIndex="1"/>
                                                                        <Label text="Db" GridPane.rowIndex="2"/>
                                                                    </children>
                                                                </GridPane>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </TitledPane>
                                                <TitledPane animated="false" text="Actions">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0">
                                                            <children>
                                                                <GridPane layoutX="128.0" layoutY="-1.0"
                                                                          prefHeight="88.0" prefWidth="796.0"
                                                                          AnchorPane.bottomAnchor="0.0"
                                                                          AnchorPane.leftAnchor="0.0"
                                                                          AnchorPane.rightAnchor="0.0"
                                                                          AnchorPane.topAnchor="0.0">
                                                                    <columnConstraints>
                                                                        <ColumnConstraints hgrow="SOMETIMES"
                                                                                           minWidth="10.0"
                                                                                           prefWidth="100.0"/>
                                                                        <ColumnConstraints hgrow="SOMETIMES"
                                                                                           minWidth="10.0"
                                                                                           prefWidth="100.0"/>
                                                                    </columnConstraints>
                                                                    <rowConstraints>
                                                                        <RowConstraints minHeight="10.0"
                                                                                        prefHeight="30.0"
                                                                                        vgrow="SOMETIMES"/>
                                                                        <RowConstraints minHeight="10.0"
                                                                                        prefHeight="30.0"
                                                                                        vgrow="SOMETIMES"/>
                                                                        <RowConstraints minHeight="10.0"
                                                                                        prefHeight="30.0"
                                                                                        vgrow="SOMETIMES"/>
                                                                    </rowConstraints>
                                                                    <children>
                                                                        <RadioButton fx:id="rbtMigrateProfile"
                                                                                     mnemonicParsing="false"
                                                                                     selected="true"
                                                                                     text="Migrate Profile">
                                                                            <toggleGroup>
                                                                                <ToggleGroup fx:id="migration"/>
                                                                            </toggleGroup>
                                                                        </RadioButton>
                                                                        <RadioButton fx:id="rbtMigrateSocial"
                                                                                     mnemonicParsing="false"
                                                                                     text="Migrate Social"
                                                                                     toggleGroup="$migration"
                                                                                     GridPane.rowIndex="1"/>
                                                                    </children>
                                                                </GridPane>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </TitledPane>
                                            </panes>
                                        </Accordion>
                                    </content>
                                </Tab>
                                <Tab text="Profile Migration Options">
                                    <content>
                                        <AnchorPane prefHeight="200.0" prefWidth="200.0">
                                            <children>
                                                <ListView fx:id="lstProfileScripts"
                                                          layoutX="38.0"
                                                          layoutY="23.0" prefHeight="177.0" prefWidth="1022.0"
                                                          AnchorPane.bottomAnchor="0.0"
                                                          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
